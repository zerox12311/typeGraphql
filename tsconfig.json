{
    "compileOnSave": true,
    "compilerOptions": {
        "outDir": "build",
        "module": "commonjs",
        "target": "es2016",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "lib": [
            "es2016",
            "esnext.asynciterable"
        ]
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}